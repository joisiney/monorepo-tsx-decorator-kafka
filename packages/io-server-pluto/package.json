{
  "name": "@olympus/io-server-pluto",
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "Joisiney Leandro",
  "license": "MIT",
  "scripts": {
    "start": "echo 'No start script defined'",
    "start:dev": "echo 'No start:dev script defined'",
    "build": "rm -rf dist && tsc && tsc-alias",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "reset": "rm -rf ./dist && rm -rf node_modules && rm -rf yarn.lock && rm -fr tsconfig.tsbuildinfo",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "*exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.js"
    }
  }
}
