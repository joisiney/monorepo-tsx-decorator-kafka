{
  "name": "@olympus/fe-rn-atena",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:dev": "EXPO_USE_METRO_WORKSPACE_ROOT=1 yarn expo start",
    "android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 yarn expo run:android",
    "ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 yarn expo run:ios",
    "prebuild": "EXPO_USE_METRO_WORKSPACE_ROOT=1 yarn expo prebuild",
    "clean:gradlew": "EXPO_USE_METRO_WORKSPACE_ROOT=1 && cd android && ./gradlew clean",
    "web": "expo start --web",
    "lint": "eslint ./ --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "reset": "rm -rf ios && rm -rf android && rm -rf node_modules && rm -rf yarn.lock && rm -fr tsconfig.tsbuildinfo",
    "build": "echo 'No build script found'",
    "start": "echo 'No build script found'",
    "clean": "echo 'No build script found'"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "classnames": "^2.5.1",
    "expo": "~50.0.6",
    "expo-font": "~11.10.2",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-random-uuid": "^0.1.4",
    "tailwindcss": "3.3.2",
    "@olympus/gateway-eros": "*",
    "@olympus/lib-hera": "*",
    "@olympus/domain-ceos": "*",
    "vite": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "^18.2.55",
    "@types/react-native": "^0.73.0"
  },
  "private": true
}
